{
    "openapi": "3.0.0",
    "info": {
      "title": "API de Ejemplo",
      "version": "1.0.0",
      "description": "Documentaci√≥n de API con Swagger"
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Servidor local"
      }
    ],
    "tags": [
      {
        "name": "Usuarios",
        "description": "Operaciones relacionadas con usuarios"
      },
      {
        "name": "Productos",
        "description": "Operaciones relacionadas con productos"
      }
    ],
    "paths": {
      "/usuarios": {
        "get": {
          "tags": ["Usuarios"],
          "summary": "Obtener lista de usuarios",
          "responses": {
            "200": {
              "description": "Lista de usuarios",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Usuario"
                  }
                }
              }
            }
          }
        }
      },
      "/productos": {
        "get": {
          "tags": ["Productos"],
          "summary": "Obtener lista de los productos",
          "responses": {
            "200": {
              "description": "Lista de productos",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Productos"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Usuario": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "example": 1
            },
            "nombre": {
              "type": "string",
              "example": "Jean Garibay"
            },
            "email": {
              "type": "string",
              "example": "Jean.garibay@example.com"
            }
          }
        },
        "Productos": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "example": 1
              },
              "nombre": {
                "type": "string",
                "example": "Monitor"
              },
              "Marca": {
                "type": "string",
                "example": "Asus"
              }
            }
          }
      }
    }
  }
  